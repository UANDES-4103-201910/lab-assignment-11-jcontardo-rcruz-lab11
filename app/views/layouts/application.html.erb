<!DOCTYPE html>
<html>
  <head>
    <title>Lab11 CONTARDO CRUZ</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head> 
  <body>
      <nav class="navbar navbar-default" style="background-color:#75b0ec;">
      <div class="container-fluid">

        <div class="nav navbar-nav">
          <button style="background-color:#75b0ec;border-style:none; "><%= link_to('Home', :root) %></button>
          <button style="background-color:#75b0ec;border-style:none;"><%= render 'devise/menu/registration_items' %></button>
          <button style="background-color:#75b0ec;border-style:none;"><%= render 'devise/menu/login_items' %></button>
          <button style="background-color:#75b0ec;border-style:none;"><%= render 'shopping_carts/shopping_cart' %></button>

          <div>
            <p class="notice"><%= notice %></p>
          </div>
          <div>
            <p class="alert"><%= alert %></p>
          </div>
        </div>
      </div>
    </nav>




    
    <%= yield %>
  </body>
</html>